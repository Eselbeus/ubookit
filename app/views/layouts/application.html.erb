<!DOCTYPE html>
<html>
  <head>
    <title>Ubookit</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render partial: "shared/navbar" %>
    <% if logged_user? %>
      <h1>Welcome Back! <%= current_user.name%></h1>
    <% end %>
    <%= yield %>
  </body>
</html>
